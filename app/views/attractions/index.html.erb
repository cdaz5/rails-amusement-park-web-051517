<h1> Listing Attractions</h1>
  <table cellspacing='20'>
    <tr>
      <th>Name</th>
      <th>&nbsp;</th>
      <th>Tickets</th>
      <th>&nbsp;</th>
      <th>Details</th>
    </tr>
    <% @attractions.each do |attraction| %>
    <tr>
      <td> <%= attraction.name %> </td><td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
      <td> <%= attraction.tickets %> </td><td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>
      <td> <%= link_to "#{attraction.name}", attraction_path(attraction) %> </td>
    </tr>
    <% end %>
  </table>
<br>
  <%= link_to "Create Attraction", new_attraction_path %>
